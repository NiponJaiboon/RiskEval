<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="iSabaya.UserPersonalization,iSabaya" table="UserPersonalization">
    <!-- Primary key-->
    <id name="ID" unsaved-value="0">
      <generator class="native"/>
    </id>
    <property name="PageID" />
    <property name="Personalization" />
    <many-to-one name="User" column="UserID" class="iSabaya.User,iSabaya" fetch="join"/><!--by kittikun 20140520-->
  </class>
</hibernate-mapping>