<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="iSabaya.SystemUser,iSabaya" table="SystemUser">
    <!-- Primary key-->
    <id name="SystemUserID" access="field.camelcase" unsaved-value="0">
      <generator class="native"/>
    </id>
    <property name="SystemID" type="iSabaya.SystemEnum,iSabaya.Config"/>
    <component name="EffectivePeriod"   >
      <property name="From" column="EffectiveFrom"/>
      <property name="To" column="EffectiveTo"/>
    </component>
    <property name="IsDisable"  />
    <many-to-one name="User" column="UserId" fetch="join"/>
  </class>
</hibernate-mapping>


